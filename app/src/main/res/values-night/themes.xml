<resources>
    <!-- Base application theme. -->
    <style name="Theme.SeeOther" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/red_200</item>
        <item name="colorPrimaryVariant">@color/red_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/pink_200</item>
        <item name="colorSecondaryVariant">@color/pink_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- ActionBar title text color -->
        <item name="android:textColorPrimary">@color/white</item>
        <!-- ActionBar subtitle text color -->
        <item name="android:textColorSecondary">@color/white</item>
        <!-- ActionBar background color -->
        <item name="colorSurface">@color/red_500</item>
        <!-- Customize your theme here. -->
    </style>
</resources>